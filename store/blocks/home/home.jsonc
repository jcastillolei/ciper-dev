    "blocks": ["product-summary.shelf#home"],
    "children": ["slider-layout#shelf-home"],
    "props": {
      "maxItems": 25
    }
  },
  "slider-layout#shelf-home": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 2
      },
      "infinite": true,
      "fullWidth": false,
      "blockClass": "shelf-home-slider"
    }
  },

  "flex-layout.row#banner-home": {
    "children": ["image#banner-home"],
    "props": {
      "blockClass": "banner-home"
    }
  },
  "image#banner-home": {
    "props": {
      "src": "https://ciper.vteximg.com.br/arquivos/banner-home-ciper.jpg",
      "width": "100%"
    }
