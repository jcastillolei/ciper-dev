      //"rich-text#question",
      //"rich-text#link"
    ]
  },

  "list-context.image-list#demo": {
    "children": ["slider-layout#demo-images"],
    "props": {
      "height": 720,
      "images": [
        {
          "image": "https://storecomponents.vteximg.com.br/arquivos/banner-principal.png",
          "mobileImage": "https://storecomponents.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
        },
        {
          "image": "https://storecomponents.vteximg.com.br/arquivos/banner.jpg",
          "mobileImage": "https://storecomponents.vteximg.com.br/arquivos/banner-principal-mobile.jpg"
        }
      ]
    }
  },
  "slider-layout#demo-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  },
  "flex-layout.row#buscador-ciper":{
    "children": ["flex-layout.col#buscador-left","flex-layout.col#buscador-middle","flex-layout.col#buscador-right"],
    "props": {
      "blockClass": "buscador-ciper"
    }
  },
  "rich-text#title-shelf-buscador": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "¿QUÉ NECESITAS?",
      "textColor": "heading--help-message",
      "font": "t-heading-3",
      "blockClass": "title-shelf-custom-need"
    }
  },
  "flex-layout.col#buscador-left":{
    "children": ["image#buscador-left","rich-text#title-busca-tu-vehiculo-left","search-bar#marca"],
    "props": {
      "blockClass": "buscador-ciper-left",
      "width": "30%"
    }
  },
  "image#buscador-left": {
    "props": {
      "src": "https://image.flaticon.com/icons/png/512/168/168466.png"
    }
  },
  "rich-text#title-busca-tu-vehiculo-left": {
    "props": {
      "textAlignment": "LEFT",
      "textPosition": "LEFT",
      "text": "Busca tu vehículo",
      "textColor": "heading--help-message",
      "blockClass": "title-shelf-custom-single"
    }
  },
  "search-bar#marca": {
    "blocks": ["autocomplete-result-list.v2"],
    "props": {
      "openAutocompleteOnFocus": true,
      "placeholder": "Busca tu vehículo"
    }
  },

  "flex-layout.col#buscador-middle":{
    "children": ["image#buscador-middle","rich-text#title-busca-tu-vehiculo-middle","search-bar#sku"],
    "props": {
      "blockClass": "buscador-ciper-middle",
      "width": "30%"
    }
  },
  "image#buscador-middle": {
    "props": {
      "src": "https://image.flaticon.com/icons/png/512/58/58274.png"
    }
  },
